{
    "env": {
        "es2020": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "warn",
        "react/react-in-jsx-scope": "warn",
        "react/jsx-one-expression-per-line": "off",
        "react/jsx-filename-extension": "off",
        "comma-dangle": "warn",
        "arrow-parens": "warn",
        "object-curly-newline": "warn"
    }
}
